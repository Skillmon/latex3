\TESTEXP{pass~ in~ several~ tokens}
  {
    \keyval_parse:nnn
      { \KV_check_key:nn { key } }
      { \KV_check_key_val:nnnn { key } { val } }
      { k = v, k, key=val, key }
    \NEWLINE
    \use:e
      {
        \keyval_parse:nnn
          { \KV_check_key:nn { key } }
          { \KV_check_key_val:nnnn { key } { val } }
          { k = v, k, key=val, key }
      }
  }

